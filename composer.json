{
    "name": "benq/laravel_http_logger",
    "type": "library",
    "description": "Laravel HTTP logging library",
    "keywords": ["laravel", "http", "log", "json"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tony H Huang",
            "email": "Tony.H.Huang@BenQ.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Benq\\Logger\\": "src/Logger"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "require-dev": {
        "monolog/monolog": "^1.1|^2.0",
        "phpunit/phpunit": "^8.5",
        "pragmarx/random": "^0.2.2"
    },
     "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Benq\\Logger\\Providers\\LoggerServiceProvider"
            ]
        }
    }
}
